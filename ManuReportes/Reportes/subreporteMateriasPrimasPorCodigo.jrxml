	<parameter name="piezas_codigo" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
	Materia_Prima_Tipo.tipo as tipo,
	Materia_Prima.espesor as espesor,
	Calidad_Tipo.calidad as calidad,
	Materia_Prima.numero_remito, Materia_Prima.n_certificado

FROM
     Piezas
     inner join Materia_Prima_Pieza
     inner join Materia_Prima
     inner join Materia_Prima_Tipo
     on Materia_Prima_Tipo.id_tipo=Materia_Prima.id_tipo
     on Materia_Prima.id_materia_prima=Materia_Prima_Pieza.id_materia_prima
     on Piezas.codigo=Materia_Prima_Pieza.codigo_pieza
     inner join Calidad_Tipo
     on Calidad_Tipo.id_calidad=Materia_Prima.id_calidad
WHERE
     Piezas.codigo =$P{piezas_codigo}
     and Piezas.activado=1
     and Materia_Prima.activado=1
     and Materia_Prima_Pieza.activado=1
group by
    Materia_Prima_Tipo.tipo,
	Materia_Prima.espesor,
	Calidad_Tipo.calidad,
	Materia_Prima.numero_remito,
	Materia_Prima.n_certificado]]>
	</queryString>
